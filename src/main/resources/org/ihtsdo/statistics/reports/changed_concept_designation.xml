<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<reportConfig>
	<name>Changed Concept Designation</name>
	<inputFile>STATEDROOTDESC</inputFile>
	<inputFile>CONCEPTS</inputFile>
	<inputFile>CONCEPTS_PREVIOUS</inputFile>
	<inputFile>DESCRIPTIONS</inputFile>
	<inputFile>DESCRIPTIONS_PREVIOUS</inputFile>
	<inputFile>TCLOSURESTATED</inputFile>
	<inputFile>EXT_LANGUAGE</inputFile>
	<storedProcedure>
		<name>createChangedConceptDesignationStatistics(?)</name>
		<param><name>releaseDate</name><order>1</order><sqlType>INT</sqlType></param>
	</storedProcedure>
	<outputFile>
	<file>STATISTICS_ON_EXISTING_CONCEPTS_DESIGNATION_count.csv</file>
	<reportHeader>id,term,AnyChange,ChangedFSN,DescriptionPointToSource,RetiredDescription,NewDescription,ReactivatedDescription</reportHeader>
	<select>
	<tableName>cptRootLevel_table7</tableName>
	</select>
	<select>
	<tableName>cptFirstLevel_table7</tableName>
	</select>
	</outputFile>
	<outputFile>
	<file>STATISTICS_ON_EXISTING_CONCEPTS_DESIGNATION_percent.csv</file>
	<reportHeader>id,term,AnyChange,PercOfChangedFSN,PercOfDescriptionPointToSource,PercOfRetiredDescriptions,PercOfNewDescriptions,PercOfReactivatedDescription</reportHeader>
	<select>
	<tableName>cptRootLevel_table8</tableName>
	</select>
	<select>
	<tableName>cptFirstLevel_table8</tableName>
	</select>
	</outputFile>
	<outputDetailFile>
	<file>changedConceptDesignationDetails.csv</file>
	<reportHeader>Hierarchy,Id,Term,ChangeType,DescriptionId,Priority</reportHeader>
	<sctIdIndex>1</sctIdIndex>
	<excluyentListPriority>40</excluyentListPriority>
	<priorityListColumnIndex>5</priorityListColumnIndex>
	<storedProcedure>
	<name>getChangedConceptDesignationDetails()</name>
	</storedProcedure>
	<createInterestConceptList>true</createInterestConceptList>
	<reportListeners>
		<reportListenerDescriptor>
		<executionClass>com.termmed.statistics.reportlisteners.ChangedDescriptionsInLangRefset</executionClass>
		<file>DescriptionsInFSNChangedConcepts.csv</file>
		<excluyentListPriority>38</excluyentListPriority>
		<sctIdIndex>1</sctIdIndex>
		<filter>
		<fieldIndex>3</fieldIndex>
		<fieldValue>FSN change</fieldValue>
		</filter>
		</reportListenerDescriptor>
		<reportListenerDescriptor>
		<executionClass>com.termmed.statistics.reportlisteners.ChangedDescriptionsInSimpleRefsetConcepts</executionClass>
		<file>ChangedDescriptionsInSimpleRefsetConcepts.csv</file>
		<excluyentListPriority>37</excluyentListPriority>
		<sctIdIndex>1</sctIdIndex>
		<filter>
		<fieldIndex>3</fieldIndex>
		<fieldValue>FSN change</fieldValue>
		</filter>
		</reportListenerDescriptor>
		<reportListenerDescriptor>
		<executionClass>com.termmed.statistics.reportlisteners.ChangedDescriptionsInStatedTargets</executionClass>
		<file>ChangedDescriptionsInStatedTargets.csv</file>
		<excluyentListPriority>39</excluyentListPriority>
		<sctIdIndex>1</sctIdIndex>
		<filter>
		<fieldIndex>3</fieldIndex>
		<fieldValue>FSN change</fieldValue>
		</filter>
		</reportListenerDescriptor>
	</reportListeners>
	</outputDetailFile>
